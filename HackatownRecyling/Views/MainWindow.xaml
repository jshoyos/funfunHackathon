<Window x:Class="HackatownRecyling.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HackatownRecyling"
        mc:Ignorable="d"
        Title="Recycle" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>

        <Button Command="{Binding UploadCommand}" Margin="25"  Content="Browse..."/>
        <Button Grid.Column="1" Command="{Binding LiveCameraCommand}" Margin="25" Content="Live Camera"/>
    </Grid>
</Window>
